{
  "expo": {
    "name": "AtheX Wallet",
    "slug": "athex-demo-wallet",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/wallet.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/wallet.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.athex_web3.athexdemowallet"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/wallet.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "9cb30944-663a-4b5b-8ec2-2bcb4ac8ab3c"
      }
    },
    "owner": "athex_web3"
  }
}
